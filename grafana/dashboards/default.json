          "pluginVersion": "4.11.4",
          "queryType": "table",
          "rawSql": "SELECT\n  Timestamp,\n  TraceId,\n  JSONExtractString(SpanAttributes['http.request.body'], 'players') as players,\n  JSONExtractString(SpanAttributes['http.request.body'], 'board') as board,\n  JSONExtractString(JSONExtractString(SpanAttributes['http.request.body'], 'options'), 'mode') as mode,\n  JSONExtractString(JSONExtractString(SpanAttributes['http.request.body'], 'options'), 'iterations') as mc_iterations,\n  Duration / 1e6 as duration_ms\nFROM\n  observability.otel_traces\n\nWHERE\n  SpanName like 'HTTP POST /poker/equity/calculate' \n  AND mapContains(SpanAttributes, 'http.request.body')\nORDER BY Timestamp DESC\nLIMIT\n  1000",
