          "pluginVersion": "4.11.4",
          "queryType": "table",
          "rawSql": "SELECT\n  Timestamp,\n  TraceId,\n  SpanId,\n  ParentSpanId,\n  SpanName,\n  ServiceName,\n  SpanAttributes['http.request.body'] as requestBody,\n  SpanAttributes,\n  Duration\nFROM\n  observability.otel_traces\nwhere\n  SpanName like 'HTTP POST /poker/%'\nORDER BY\n  Timestamp DESC\nLIMIT\n  1000",
