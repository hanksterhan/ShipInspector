{
    "name": "server",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "start": "dotenv -e .env -- npm run istart",
        "istart": "npm run build && ts-node dist/server/src/index.js",
        "build": "tsc && tsc-alias -p tsconfig.json && tsc -p ../common/tsconfig.json && npm run copy-assets",
        "watch": "nodemon --watch src --exec \"npm run start\" \"npm run copy-assets\"",
        "lint": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "copy-assets": "copyfiles -u 1 src/docs/*.yaml dist/",
        "generate-metrics": "node scripts/generate-metrics-data.js",
        "seed:equity-cache": "ts-node scripts/seed-equity-cache.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "jest": {
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest"
        },
        "testEnvironment": "node",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json",
            "node"
        ],
        "testMatch": [
            "**/src/**/*.spec.(ts|tsx)"
        ],
        "moduleNameMapper": {
            "^@common/(.*)$": "<rootDir>/../common/src/$1"
        }
    },
    "dependencies": {
        "@opentelemetry/api": "^1.9.0",
        "@opentelemetry/api-logs": "^0.55.0",
        "@opentelemetry/auto-instrumentations-node": "^0.47.0",
        "@opentelemetry/exporter-logs-otlp-http": "^0.55.0",
        "@opentelemetry/exporter-metrics-otlp-http": "^0.55.0",
        "@opentelemetry/exporter-trace-otlp-http": "^0.55.0",
        "@opentelemetry/instrumentation": "^0.55.0",
        "@opentelemetry/instrumentation-express": "^0.42.0",
        "@opentelemetry/instrumentation-http": "^0.55.0",
        "@opentelemetry/resources": "^1.25.0",
        "@opentelemetry/sdk-logs": "^0.208.0",
        "@opentelemetry/sdk-metrics": "^1.25.0",
        "@opentelemetry/sdk-node": "^0.55.0",
        "@opentelemetry/sdk-trace-base": "^1.25.0",
        "@opentelemetry/sdk-trace-node": "^1.25.0",
        "@opentelemetry/semantic-conventions": "^1.25.0",
        "@pyroscope/nodejs": "^0.4.8",
        "axios": "^1.3.1",
        "better-sqlite3": "^12.5.0",
        "cheerio": "^1.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "tsconfig-paths": "^4.2.0",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.11",
        "@types/chai": "^4.3.17",
        "@types/cheerio": "^0.22.35",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.1.0",
        "@types/sinon": "^17.0.3",
        "@types/swagger-jsdoc": "^6.0.4",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/uuid": "^10.0.0",
        "chai": "^5.1.1",
        "copyfiles": "^2.4.1",
        "dotenv-cli": "^7.4.2",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "glob": "^13.0.0",
        "jest": "^29.7.0",
        "mocha": "^10.7.3",
        "nodemon": "^3.1.4",
        "prettier": "^3.3.3",
        "sinon": "^18.0.0",
        "ts-jest": "^29.2.4",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.16",
        "typescript": "^5.5.4"
    }
}
